{
	"name": "tenantcloud/rentler-sdk",
	"description": "Rentler(c) laravel partner API SDK.",
	"minimum-stability": "stable",
	"license": "MIT",
	"require": {
		"php": ">=7.4 || ^8.0",
		"ext-json": "*",
		"illuminate/support": "^8.0",
		"spatie/macroable": "1.0.1",
		"guzzlehttp/guzzle": "~7.2",
		"tenantcloud/php-standard": "^1.2.3",
		"tenantcloud/data-transfer-objects": "^1.0",
		"tenantcloud/guzzle-helper": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"orchestra/testbench": "^6.0",
		"friendsofphp/php-cs-fixer": "~2.18.2",
		"tenantcloud/php-cs-fixer-rule-sets": "~1.1.0"
	},
	"autoload": {
		"psr-0": {
			"": "src/"
		},
		"files": [
			"src/TenantCloud/RentlerSDK/functions.php"
		]
	},
	"repositories": [
		{
			"type": "git",
			"url": "https://github.com/tenantcloud/php-data-transfer-objects"
		},
		{
			"type": "git",
			"url": "https://github.com/tenantcloud/guzzle-helper"
		}
	],
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit",
		"cs-fix": "./tools/php-cs-fixer/vendor/bin/php-cs-fixer fix -v --show-progress=estimating",
		"phpstan":  "./tools/phpstan/vendor/bin/phpstan analyse"
	},
	"extra": {
		"laravel": {
			"providers": [
				"TenantCloud\\RentlerSDK\\RentlerSDKServiceProvider"
			]
		}
	}
}
